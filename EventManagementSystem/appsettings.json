{
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_URL}"
  },
  "EmailSettings": {
    "SmtpServer": "${SMTP_SERVER}",
    "Port": "${SMTP_PORT}",
    "SenderName": "${SENDER_NAME}",
    "SenderEmail": "${SENDER_EMAIL}",
    "Password": "${EMAIL_PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}